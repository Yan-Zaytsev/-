#include <iostream>

using namespace std;

int main()
{
   double a1, b1, c1, a2, b2, c2, d;
   cout << "A1= ";
   cin >> a1;
   cout << "B1= ";
   cin >> b1;
   cout << "C1= ";
   cin >> c1;
   cout << "A2= ";
   cin >> a2;
   cout << "B2= ";
   cin >> b2;
   cout << "C2= ";
   cin >> c2;
   d=(a1*b2)-(a2*b1);
   
   cout << "x= " << ((c1*b2)-(c2*b2))/d << endl;
   cout << "y= " << ((a1*c2)-(a2*c1))/d << endl;
   return 0;
   
}
